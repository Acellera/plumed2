MOLINFO STRUCTURE=helix.pdb
ALPHARMSD RESIDUES=all TYPE=DRMSD LESS_THAN=(SPLINE R_0=0.08 NN=8 MM=12) LABEL=a
ALPHARMSD RESIDUES=all TYPE=DRMSD LESS_THAN=(SPLINE R_0=0.08 NN=8 MM=12) NUMERICAL_DERIVATIVES LABEL=anum
ANTIBETARMSD RESIDUES=all TYPE=DRMSD STRANDS_CUTOFF=1.0 LESS_THAN=(SPLINE R_0=0.08 NN=8 MM=12) LABEL=b
ANTIBETARMSD RESIDUES=all TYPE=DRMSD LESS_THAN=(SPLINE R_0=0.08 NN=8 MM=12) NUMERICAL_DERIVATIVES LABEL=bnum
PARABETARMSD RESIDUES=all TYPE=DRMSD STRANDS_CUTOFF=1.0 LESS_THAN=(SPLINE R_0=0.08 NN=8 MM=12) LABEL=p
PARABETARMSD RESIDUES=all TYPE=DRMSD LESS_THAN=(SPLINE R_0=0.08 NN=8 MM=12) NUMERICAL_DERIVATIVES LABEL=pnum
PRINT ARG=a.*,b.*,p.* STRIDE=1 FILE=colvar FMT=%8.4f
DUMPDERIVATIVES ARG=a.*,anum.*,b.*,bnum.*,p.*,pnum.* FILE=derivatives FMT=%8.4f STRIDE=1
