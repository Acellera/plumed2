MOLINFO STRUCTURE=helix.pdb
ALPHARMSD BACKBONE=all TYPE=DRMSD LESS_THAN=(SPLINE R_0=0.08 NN=8 MM=12) LABEL=a
ALPHARMSD BACKBONE=all TYPE=DRMSD LESS_THAN=(SPLINE R_0=0.08 NN=8 MM=12) NUMERICAL_DERIVATIVES LABEL=anum
ANTIBETARMSD BACKBONE=all TYPE=OPTIMAL LESS_THAN=(SPLINE R_0=0.08 NN=8 MM=12) STRANDS_CUTOFF=1.0 LABEL=b
ANTIBETARMSD BACKBONE=all TYPE=OPTIMAL LESS_THAN=(SPLINE R_0=0.08 NN=8 MM=12) STRANDS_CUTOFF=1.0 NUMERICAL_DERIVATIVES LABEL=bnum
PRINT ARG=a.*,b.* STRIDE=1 FILE=colvar FMT=%8.4f
DUMPDERIVATIVES ARG=a.*,anum.*,b.*,bnum.* FILE=derivatives FMT=%8.4f STRIDE=1
